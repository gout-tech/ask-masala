pipeline {
    agent any
    environment {
    PATH = "/usr/share/maven/bin:$PATH"
    }
    stages {
        stage('git checkout') {
            steps {
                git 'https://github.com/gout-tech/ask-masala'
            }
        }
        stage('Maven Build') {
            steps {
                sh "mvn clean package"
            }
        }   
    }
}
